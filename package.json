{
  "name": "ginjs",
  "version": "0.0.4",
  "description": "A tool for building feature-oriented productlines with Node.js.",
  "main": "index.js",
  "scripts": {
    "compile": "babel -d lib/ src/;",
    "buildDoc": "jsdoc -c jsdoc.json;",
    "flow": "flow; test $? -eq 0 -o $? -eq 2;",
    "jest": "jest;",
    "test": "npm run flow && npm run compile && npm run jest;",
    "deploy": "npm run test && npm run buildDoc;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WiRai/ginjs.git"
  },
  "author": "WiRai",
  "bin": {
    "gin": "./bin/gin.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^14.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsdoc": "^2.3.1",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "flow-bin": "^0.30.0",
    "jest-cli": "^14.1.0",
    "jsdoc": "^3.4.0",
    "jsdoc-babel": "^0.2.1"
  },
  "dependencies": {
    "fs-extra": "^0.30.0",
    "nunjucks": "^2.4.2",
    "superimp": "^0.0.4"
  },
  "preferGlobal": "true"
}
